{
  "name": "<%= kebabName %>",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build": "gulp clean && gulp build",
    "shell": "node -e \"const { app } = require('./dist/app/app')\" -i",
    "test": "mocha -r source-map-support/register \"./dist/**/*.spec.js\"",
    "start": "node server.js",
    "dev:app": "gulp clean && gulp dev:app",
    "dev:test": "tsc && concurrently -p \"[{name}]\" -n \"BUILD,SERVER\" -c \"bold\" \"tsc -w\" \"mocha -r source-map-support/register -w \"./dist/**/*.spec.js\"\""
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@types/node": "^8.0.47",
    "@foal/ajv": "^0.4.0-beta.3",
    "@foal/core": "^0.4.0-beta.3",
    "source-map-support": "^0.5.1"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "typescript": "^2.7.2",
    "gulp-typescript": "^4.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.4"
  }
}