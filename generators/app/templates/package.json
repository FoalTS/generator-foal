    "helmet": "^3.9.0",
    "@types/node": "^8.0.47",
    "@foal/ajv": "^0.4.0-alpha.3",
    "@foal/common": "^0.4.0-alpha.3",
    "@foal/core": "^0.4.0-alpha.3",
